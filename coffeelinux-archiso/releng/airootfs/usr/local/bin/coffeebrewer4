printf "%b" "\n3 - Intel\n"
printf "%b" "\n4 - Virtual GPU (VMware etc.)\n"
read -r gpucheck
case $gpucheck in
  1) gpu01="amd" ;;
  2) gpu01="nvidia" ;;
  3) gpu01="intel" ;;
  4) gpu01="vm" ;;
